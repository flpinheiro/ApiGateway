        }
    },
    "SwaggerEndPoints": [{
            "Key": "wscadastro",
            "Config": [{
                "Name": "WsCadastro API",
                "Version": "v1",
                "Url": "http://10.0.18.30:5051/WsCadastro/v2/api-docs"
            }]
        },
        {
            "Key": "uaa",
            "Config": [{
                "Name": "Uaa API",
                "Version": "v2",
                "Url": "http://10.0.18.30:5052/v2/api-docs"
            }]
        },
        {
            "Key": "WeatherForecast",
            "Config": [{
                "Name": "WeatherForecast API",
                "Version": "v1",
                "Url": "http://webclientconsul/swagger/v1/swagger.json"
            }]
        }
    ]
}